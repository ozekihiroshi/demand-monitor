# 対象URL
BASE_URL=https://mobile.ceri.link

# 逆プロキシ/名前解決が必要なときのみ
# HOST_HEADER=mobile.ceri.link
# RESOLVE_HOST=mobile.ceri.link
# RESOLVE_ADDR=127.0.0.1
# INSECURE=0   # 自己署名などでTLS検証を無効化するなら 1

# セッションクッキー名の検出（既定は *_session / -session / session）
# SESSION_COOKIE_RE='(^|[_-])session$'
# demandmonitor-session を明示したい場合だけ↓
# SESSION_COOKIE_RE='(^|[_-])demandmonitor-session$'

# ---- 資格情報（テストで使うアカウント） ----
# ※ Seeder に合わせてメールを調整してください
SUPERADMIN_EMAIL=super@example.com
SUPERADMIN_PASS=password

# エンジニア相当（例：pm@example.com を割当）
ENGINEER_EMAIL=pm@example.com
ENGINEER_PASS=password

# 会社担当
COMPANY_EMAIL=co@example.com
COMPANY_PASS=Password1!

# 任意: オペレータ（権限を絞ったユーザが居る場合）
OPERATOR_EMAIL=operator@example.com
OPERATOR_PASS=Password1

# 任意: プロ（電管）コンソールを使う場合のみ設定
PRO_ADMIN_EMAIL=pro-admin@example.com
PRO_ADMIN_PASS=Password1
PRO_VIEWER_EMAIL=pro-viewer@example.com
PRO_VIEWER_PASS=Password1

# 可変識別子（TSVの ${VAR} 展開で利用）
COMPANY_SLUG=demo-co
PROVIDER_SLUG=demo
FACILITY_ID=1
METER_CODE=d100318

# ツール（必要なら）
# JQ_BIN=jq
